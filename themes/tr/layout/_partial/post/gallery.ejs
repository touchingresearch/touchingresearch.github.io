<% if (post.photos && post.photos.length) { %>
  <div class="post-gallery">
    <div class="post-gallery-photos">
      <% post.photos.forEach(function(photo) { %>
        <a href="<% url_for(photo) %>" class="post-gallery-img">
          <img src="<% url_for(photo) %>" alt="">
        </a>
      <% }) %>
    </div>
  </div>
<% } %>
